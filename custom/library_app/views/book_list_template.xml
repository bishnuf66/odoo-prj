<odoo>
<template id="book_list_template" name="Book Lits">
<div id="warp" class="container">
<h1>Books</h1>
<t t-foreach="books" t-as="books">
<div class="row">
<span t-field="book.name"/>
<span t-field="book.date_published"/>
<span t-field="book.publisher_id"/>
</div>
</t>
</div>

</template>
</odoo>
