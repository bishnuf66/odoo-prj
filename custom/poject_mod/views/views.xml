<odoo>
<data>
<record id="inherited_project_model_view_form" model="ir.ui.view">
    <field name="name">inherited.project.form</field>
    <field name="model">project.project</field>
    <field name="inherit_id" ref="project.edit_project"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='user_id']" position="before">
          <field name="bishnu"/>
        </xpath>
    </field>
</record>



<record id="inherited_project_model_view_tree" model="ir.ui.view">
    <field name="name">inherited.project.tree</field>
    <field name="model">project.project</field>
    <field name="inherit_id" ref="project.view_project"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='name']" position="before">
          <field name="bishnu"/>
        </xpath>
    </field>
</record>
</data>
</odoo>